@model IEnumerable<EntityChangeTracking_AspNetCore_3._0.Models.Example>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FieldA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FieldD)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FieldA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FieldB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FieldC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FieldD)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
